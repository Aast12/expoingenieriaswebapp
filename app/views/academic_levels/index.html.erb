<p id="notice"><%= notice %></p>

<h1>Academic Levels</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @academic_levels.each do |academic_level| %>
      <tr>
        <td><%= academic_level.name %></td>
        <td><%= link_to 'Show', academic_level %></td>
        <td><%= link_to 'Edit', edit_academic_level_path(academic_level) %></td>
        <td><%= link_to 'Destroy', academic_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Academic Level', new_academic_level_path %>
