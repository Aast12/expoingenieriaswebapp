<div class="content">
  <h1>Proyectos</h1>
  <br />
  <div class="row">
    <div class="col-md-3">
      <div class="list-group">
        <a class="list-group-item list-group-item-action active" id="list-project-name" data-toggle="list" href="#list-project-name" role="tab" aria-controls="name">Nombre</a>
        <a class="list-group-item list-group-item-action" id="list-project-sore" data-toggle="list" href="#list-project-score" role="tab" aria-controls="score">Calificaci√≥n</a>
      </div>
    </div>
    <div class="col-md-8">
      <% @projects.each do |project| %>
        <%= render "projects/project_card", project: project %>
      <% end %>
    </div>
  </div>
  <div class="content-actions">
    <%= link_to 'Nuevo proyecto', new_project_path, :class => "btn btn-light" %>
  </div>
</div>
